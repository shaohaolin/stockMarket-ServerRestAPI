
    <div class="large-2 columns">
        <p></p>
    </div>
    <div class="large-6 columns">
        <h4>Place Bid Order for {{model.name}}</h4>
        <form>
            <div class="row">
                <div class="large-12 columns">
                    <label>Price
                        {{input value=model.price type="number" }}
                    </label>
                </div>
            </div>
            <div class="row">
                <div class="large-12 columns">
                    <label>Size
                        {{input value=model.size type="number" }}
                    </label>
                </div>
            </div>

        </form>
        <a href="#" class="button small radius" {{action 'addBuyOrder'}}>Save</a>
        <a href="#" class="button small radius" {{action 'cancel'}}>Cancel</a>
    </div>
    <div class="large-3 columns">
        <h1>&emsp;</h1>
        <div id="logo">
            <img {{bind-attr src="model.symbolURL"}} alt="Logo">
        </div>
    </div>
    <div class="large-2 columns">
    </div>
    <hr>
